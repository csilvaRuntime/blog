<h1>Login</h1>

<div>
      <%= form_with scope: :session, url: sessions_path, local: true do |f| %>
            <%= f.label :email %>
            <%= f.email_field :email, placeholder: "example@email.com" %>

            <%= f.label :password%>
            <%= f.password_field :password, placeholder: "************" %>
            
            <%= f.submit "Login" %>
      <% end %>
</div>
